{
    "name" : "makinacorpus/drupal-phplayout",
    "type" : "drupal-module",
    "description" : "PHP Layout library support, brings page composition capacity to Drupal",
    "homepage" : "http://github.com/makinacorpus/drupal-phplayout",
    "license" : "GPL-2.0-or-later",
    "authors" : [{
            "name" : "Pierre Rineau",
            "email" : "pierre.rineau@makina-corpus.com"
        }, {
            "name" : "Makina Corpus",
            "homepage" : "http://makina-corpus.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:makinacorpus/php-layout.git",
            "no-api": true
        }
    ],
    "autoload" : {
        "psr-4" : {
            "MakinaCorpus\\Drupal\\Layout\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "MakinaCorpus\\Drupal\\Layout\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require-dev" : {
        "phpunit/phpunit" : "^5"
    },
    "require" : {
        "makinacorpus/drupal-plusql" : "^1",
        "makinacorpus/drupal-sf-dic" : "^3.3.9 || ^4",
        "makinacorpus/php-layout" : "*",
        "php": ">=7.0"
    },
    "suggests" : {
        "makinacorpus/drupal-badm" : "Bootstrap 3.x based admin theme, suitable for using default templates",
        "makinacorpus/drupal-udashboard" : "Provides better content selectors"
    }
}
